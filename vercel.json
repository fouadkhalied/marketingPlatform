{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
    "routes": [
      { "src": "/(.*)", "dest": "/api/index.js" }
    ],
    "outputDirectory": "." 
  }
  