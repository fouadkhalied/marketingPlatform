{
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api"
      }
    ],
    "functions": {
      "api/index.ts": {
        "runtime": "@vercel/node@3.0.7"
      }
    }
  }